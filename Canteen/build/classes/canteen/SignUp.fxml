         <children>
            <TextField fx:id="textbox2" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="44.0" prefWidth="449.0" promptText="Password" styleClass="tf">
               <HBox.margin>
                  <Insets />
               </HBox.margin>
            </TextField>
         </children>
      </HBox>
      <HBox alignment="CENTER_RIGHT" layoutX="10.0" layoutY="249.0" prefHeight="45.0" prefWidth="220.0" stylesheets="@stylesheet.css">
         <children>
            <JFXButton fx:id="signupid" alignment="CENTER" onAction="#signUpPressed" prefHeight="39.0" prefWidth="120.0" ripplerFill="WHITE" styleClass="pink-btn" text="Sign Up" />
         </children>
         <VBox.margin>
            <Insets left="15.0" right="15.0" top="20.0" />
