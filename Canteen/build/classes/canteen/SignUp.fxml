      </Label>
      <HBox prefHeight="45.0" prefWidth="200.0" styleClass="tf_box" stylesheets="@stylesheet.css">
         <VBox.margin>
            <Insets left="15.0" right="15.0" top="20.0" />
         </VBox.margin>
         <children>
            <TextField fx:id="full_name_sign_up_textbox" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="44.0" prefWidth="429.0" promptText="Full Name" styleClass="tf" HBox.hgrow="ALWAYS">
               <HBox.margin>
                  <Insets />
               </HBox.margin></TextField>
         </children>
      </HBox>
      <HBox prefHeight="44.0" prefWidth="429.0" styleClass="tf_box" stylesheets="@stylesheet.css">
         <children>
            <TextField fx:id="textbox" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="44.0" prefWidth="429.0" promptText="Username" styleClass="tf" HBox.hgrow="ALWAYS">
